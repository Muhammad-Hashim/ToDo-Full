{
    
    "builds": [
      {
        "src": "/bulid",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1",
        "headers": {
          "Cache-Control": "public, max-age=3600"
        }
      }
    ]
  }
  